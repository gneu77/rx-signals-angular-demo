{
  "name": "@rx-signals/angular-demo",
  "version": "1.0.0",
  "description": "Small app to demonstrate development with rx-signals in Angular",
  "author": "Gerd Neudert",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-ci": "ng test --source-map=false --watch=false --browsers=jsdom",
    "lint": "ng lint",
    "format": "prettier --config .prettierrc --write \"src/**/*.{js,ts,html}\"",
    "prepare": "husky install",
    "server": "cpy server/book-data.json . --rename=data.json && json-server --watch server/data.json"
  },
  "dependencies": {
    "@angular/animations": "^15.1.5",
    "@angular/common": "^15.1.5",
    "@angular/compiler": "^15.1.5",
    "@angular/core": "^15.1.5",
    "@angular/forms": "^15.1.5",
    "@angular/platform-browser": "^15.1.5",
    "@angular/platform-browser-dynamic": "^15.1.5",
    "@angular/router": "^15.1.5",
    "@picocss/pico": "^1.5.7",
    "@rx-signals/angular-provider": "^3.0.0-rc9",
    "@rx-signals/store": "^3.0.0-rc39",
    "rxjs": "^7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.6",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.1.6",
    "@angular/compiler-cli": "^15.1.5",
    "@types/jasmine": "~4.3.1",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jasmine-core": "~4.5.0",
    "jsdom": "^21.1.0",
    "json-server": "^0.17.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "karma-jsdom-launcher": "^14.0.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "prettier-eslint": "^15.0.1",
    "typescript": "~4.9.5"
  },
  "lint-staged": {
    "*.{js,ts,html}": [
      "prettier --config .prettierrc --write",
      "eslint"
    ]
  },
  "volta": {
    "node": "18.12.1"
  }
}
